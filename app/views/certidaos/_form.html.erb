<div class="normal_form">
  <div class="form_titulo">
    &nbsp;
  </div>
<% form_for(@certidao) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :documento_id %><br />
    <%= f.text_field :documento_id %>
  </p>
  <p>
    <%= f.label :numero %><br />
    <%= f.text_field :numero %>
  </p>
  <p>
    <%= f.submit 'Salvar' %>
  </p>
<% end %>
</div>

