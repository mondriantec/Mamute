<h1>Editing sub_depositario</h1>

<% form_for(@sub_depositario) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :depositario_id %><br />
    <%= f.text_field :depositario_id %>
  </p>
  <p>
    <%= f.label :descricao %><br />
    <%= f.text_field :descricao %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @sub_depositario %> |
<%= link_to 'Back', sub_depositarios_path %>