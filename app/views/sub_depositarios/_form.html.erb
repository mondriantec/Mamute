<% form_for(@sub_depositario) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :descricao %><br />
    <%= f.text_field :descricao %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.check_box :status %>
  </p>
  <%= hidden_field_tag 'sub_depositario[depositario_id]', params['depositario_id'] %>
  
  <p>
    <%= f.submit 'Salvar' %>
  </p>
<% end %>
