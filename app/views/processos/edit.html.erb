<h1>Editar processo</h1>

<% form_for(@processo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :tipo_processo_id %><br />
    <%= f.text_field :tipo_processo_id %>
  </p>
  <p>
    <%= f.submit 'Salvar' %>
  </p>
<% end %>

<%= link_to 'Visualizar', @processo %> |
<%= link_to 'Voltar', processos_path %>
