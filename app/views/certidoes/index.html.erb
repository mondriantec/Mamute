<h1>Cadastro de certidaos</h1>


<br/>
<div dojoType="dijit.layout.TabContainer" style="width: 100%; height: 300px;"    tabStrip="true">

    <div dojoType="dijit.layout.ContentPane" title="Certidões">
        <%= render 'lista', :certidoes => @certidoes_emitidas %>
    </div>
                                               
    <% if usuario_final? %>
    <div dojoType="dijit.layout.ContentPane" title="Solicitar Certidão">
        <%= render 'lista', :certidoes => @certidoes_emitidas %>
    </div>               
	<% end %>


</div>



<br />

<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/dojo/1.5/dojo/dojo.xd.js" djConfig="parseOnLoad: true"></script>
<script type="text/javascript">
    dojo.require("dijit.layout.TabContainer");
    dojo.require("dijit.layout.ContentPane");
</script>

