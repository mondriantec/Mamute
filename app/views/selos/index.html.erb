<h1>Listing selos</h1>

<table>
  <tr>
    <th>Numero</th>
  </tr>

<% @selos.each do |selo| %>
  <tr>
    <td><%=h selo.numero %></td>
    <td><%= link_to 'Show', selo %></td>
    <td><%= link_to 'Edit', edit_selo_path(selo) %></td>
    <td><%= link_to 'Destroy', selo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New selo', new_selo_path %>