<h1>Cadastro de tipo_processos</h1>

<table>
  <tr>
    <th>Tipo</th>
    <th>Descricao</th>
  </tr>

<% @tipo_processos.each do |tipo_processo| %>
  <tr>
    <td><%=h tipo_processo.tipo %></td>
    <td><%=h tipo_processo.descricao %></td>
    <td><%= link_to 'Visualizar', tipo_processo %></td>
    <td><%= link_to 'Editar', edit_tipo_processo_path(tipo_processo) %></td>
    <td><%= link_to 'Apagar', tipo_processo, :confirm => 'Tem certeza?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Novo tipo_processo', new_tipo_processo_path %>
