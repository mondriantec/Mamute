<h1>Editing noticacao</h1>

<% form_for(@noticacao) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :cartorio_id %><br />
    <%= f.text_field :cartorio_id %>
  </p>
  <p>
    <%= f.label :notificado_id %><br />
    <%= f.text_field :notificado_id %>
  </p>
  <p>
    <%= f.label :motivo_notificacao_id %><br />
    <%= f.text_field :motivo_notificacao_id %>
  </p>
  <p>
    <%= f.label :texto %><br />
    <%= f.text_area :texto %>
  </p>
  <p>
    <%= f.label :data_envio_usuario %><br />
    <%= f.datetime_select :data_envio_usuario %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.label :motivo_status %><br />
    <%= f.text_area :motivo_status %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @noticacao %> |
<%= link_to 'Back', noticacaos_path %>